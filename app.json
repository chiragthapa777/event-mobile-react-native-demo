{
  "expo": {
    "name": "event-mobile",
    "slug": "event-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "src/assets/images/icon-square-dark.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "eventmobile",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "src/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-asset",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#FFFFFF",
          "image": "./assets/images/icon-circle-light.png",
          "resizeMode": "contain",
          "dark": {
            "resizeMode": "contain",
            "image": "./assets/images/icon-circle-dark.png",
            "backgroundColor": "#151718"
          }
        }
      ]
    ]
  }
}
